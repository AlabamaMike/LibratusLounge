{
  "name": "libratus-lounge",
  "version": "0.1.0",
  "description": "AI-powered poker agents for Primo Poker with varying skill levels and personalities",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "build": "tsc",
    "deploy": "wrangler deploy",
    "deploy:staging": "wrangler deploy --env staging",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:simulate": "tsx scripts/simulate.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src tests --ext .ts,.tsx",
    "lint:fix": "eslint src tests --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "prepare": "husky",
    "clean": "rimraf dist coverage .wrangler"
  },
  "keywords": [
    "poker",
    "ai",
    "cloudflare-workers",
    "texas-holdem",
    "game-ai"
  ],
  "author": "LibratusLounge Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240117.0",
    "@eslint/js": "^8.57.0",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^9.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.6.0",
    "wrangler": "^3.78.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.16.1",
    "openai": "^4.26.0"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlabamaMike/LibratusLounge.git"
  },
  "bugs": {
    "url": "https://github.com/AlabamaMike/LibratusLounge/issues"
  },
  "homepage": "https://github.com/AlabamaMike/LibratusLounge#readme"
}
